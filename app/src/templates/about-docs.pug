extends ./views/_layout.pug
block vars
  - var pageTitle = 'About Documents'
  - var bodyClass = 'about'

block content
  .page
    //- Menu Button
    button.btn.btn--nav-toggle#js-nav-toggle
      svg.inline-svg(xmlns='http://www.w3.org/2000/svg', viewBox='0 0 32 22.5')
        g.svg-menu-toggle
          path.bar(d='M20.945 8.75c0 .69-.5 1.25-1.117 1.25H3.141c-.617 0-1.118-.56-1.118-1.25s.5-1.25 1.118-1.25h16.688c.616 0 1.116.56 1.116 1.25z')
          path.bar(d='M20.923 15c0 .689-.501 1.25-1.118 1.25H3.118C2.5 16.25 2 15.689 2 15s.5-1.25 1.118-1.25h16.687c.617 0 1.118.561 1.118 1.25z')
          path.bar(d='M20.969 21.25c0 .689-.5 1.25-1.117 1.25H3.164c-.617 0-1.118-.561-1.118-1.25S2.546 20 3.164 20h16.688c.617 0 1.117.561 1.117 1.25z')
    //- Menu Button End
    //- Top Panel
    include includes/_top-panel
    //- Top Panel End
    main.content#js-top
      header.header.header--main
        .header__inner   
          //- h1.heading.heading--main О доме
          nav.nav.nav--inline
            ul.nav__list
              li.nav__item
                a(href="about-architect.html").nav__link Архитектура
              li.nav__item
                a(href="about-quality.html").nav__link Качество жизни
              li.nav__item
                a(href="about-location.html").nav__link Расположение
              
              li.nav__item.nav__item--divider
                .heading.heading--main В центре жизни
              
              li.nav__item.nav__item--active
                a(href="about-docs.html").nav__link Документы
              li.nav__item
                a(href="about-building.html").nav__link Динамика строительства
              li.nav__item
                a(href="about-news.html").nav__link Новости
      section.section
          ul.list.list--docs
            li.list__item.list__item--pdf
              a(href="#").link.link--list
                span.list__text Договор аренды земельного участка
                span.link-info
                  span.link-info__format .pdf
                  span.link-info__size (3,5 Mb)
            li.list__item.list__item--pdf
              a(href="#").link.link--list
                span.list__text ДC к договору аренды земельного участка
                span.link-info
                  span.link-info__format .pdf
                  span.link-info__size (3,5 Mb)
            li.list__item.list__item--pdf
              a(href="#").link.link--list
                span.list__text Договор участия в долевом строительстве
                span.link-info
                  span.link-info__format .pdf
                  span.link-info__size (3,5 Mb)
            li.list__item.list__item--pdf
              a(href="#").link.link--list
                span.list__text Проектная декларация
                span.link-info
                  span.link-info__format .pdf
                  span.link-info__size (3,5 Mb)
            li.list__item.list__item--pdf
              a(href="#").link.link--list
                span.list__text Заключение о соответствии проектной декларации
                span.link-info
                  span.link-info__format .pdf
                  span.link-info__size (3,5 Mb)
            li.list__item.list__item--pdf
              a(href="#").link.link--list
                span.list__text Шаблон договора уступки
                span.link-info
                  span.link-info__format .pdf
                  span.link-info__size (3,5 Mb)
            li.list__item.list__item--pdf
              a(href="#").link.link--list
                span.list__text Шаблон инвестиционного договора на приобретение машино-места
                span.link-info
                  span.link-info__format .pdf
                  span.link-info__size (3,5 Mb)
            li.list__item.list__item--pdf
              a(href="#").link.link--list
                span.list__text Изменения в устав
                span.link-info
                  span.link-info__format .pdf
                  span.link-info__size (3,5 Mb)
            li.list__item.list__item--pdf
              a(href="#").link.link--list
                span.list__text Устав
                span.link-info
                  span.link-info__format .pdf
                  span.link-info__size (3,5 Mb)
            li.list__item.list__item--pdf
              a(href="#").link.link--list
                span.list__text ОГРН
                span.link-info
                  span.link-info__format .pdf
                  span.link-info__size (3,5 Mb)
            li.list__item.list__item--pdf
              a(href="#").link.link--list
                span.list__text Разрешение на строительство
                span.link-info
                  span.link-info__format .pdf
                  span.link-info__size (3,5 Mb)
    //- Footer
    include includes/_footer
    //- Footer End
